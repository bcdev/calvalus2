<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<wps:Execute service="WPS"
             version="1.0.0"
             xmlns:wps="http://www.opengis.net/wps/1.0.0"
             xmlns:ows="http://www.opengis.net/ows/1.1"
             xmlns:xlink="http://www.w3.org/1999/xlink">

    <ows:Identifier>L2</ows:Identifier>

    <wps:DataInputs>
        <wps:Input>
            <ows:Identifier>productionName</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>example python-copy</wps:LiteralData>
            </wps:Data>
        </wps:Input>

        <wps:Input>
            <ows:Identifier>inputPath</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>/calvalus/eodata/MER_RR__1P/r03/${yyyy}/${MM}/${dd}/MER_..._1.*${yyyy}${MM}${dd}.*.N1</wps:LiteralData>
            </wps:Data>
        </wps:Input>
        <wps:Input>
            <ows:Identifier>calvalus.output.dir</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>/calvalus/examples/python/python-copy</wps:LiteralData>
            </wps:Data>
        </wps:Input>

        <wps:Input>
            <ows:Identifier>processorBundleName</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>examples-python</wps:LiteralData>
            </wps:Data>
        </wps:Input>
        <wps:Input>
            <ows:Identifier>processorBundleVersion</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>1.0</wps:LiteralData>
            </wps:Data>
        </wps:Input>
        <wps:Input>
            <ows:Identifier>processorName</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>pythoncopy</wps:LiteralData>
            </wps:Data>
        </wps:Input>

        <wps:Input>
            <ows:Identifier>minDate</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>2010-03-01</wps:LiteralData>
            </wps:Data>
        </wps:Input>
        <wps:Input>
            <ows:Identifier>maxDate</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>2010-03-01</wps:LiteralData>
            </wps:Data>
        </wps:Input>

        <wps:Input>
            <ows:Identifier>processorParameters</ows:Identifier>
            <wps:Data>
                <wps:ComplexData>
                    <parameters>
                        <prefix>copy_of_</prefix>
                    </parameters>
                </wps:ComplexData>
            </wps:Data>
        </wps:Input>
    </wps:DataInputs>

</wps:Execute>