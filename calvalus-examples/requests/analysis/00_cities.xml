<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<wps:Execute service="WPS"
             version="1.0.0"
             xmlns:wps="http://www.opengis.net/wps/1.0.0"
             xmlns:ows="http://www.opengis.net/ows/1.1"
             xmlns:xlink="http://www.w3.org/1999/xlink">

    <ows:Identifier>MA</ows:Identifier>

    <wps:DataInputs>
        <wps:Input>
            <ows:Identifier>productionName</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>example matchup 00_cities</wps:LiteralData>
            </wps:Data>
        </wps:Input>

        <wps:Input>
            <ows:Identifier>inputPath</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>/calvalus/eodata/MER_RR__1P/r03/${yyyy}/${MM}/${dd}/MER_..._1.*${yyyy}${MM}${dd}.*.N1</wps:LiteralData>
            </wps:Data>
        </wps:Input>
        <wps:Input>
            <ows:Identifier>calvalus.output.dir</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>/calvalus/projects/example/matchup/00_extraction</wps:LiteralData>
            </wps:Data>
        </wps:Input>

        <wps:Input>
            <ows:Identifier>minDate</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>2010-01-01</wps:LiteralData>
            </wps:Data>
        </wps:Input>
        <wps:Input>
            <ows:Identifier>maxDate</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>2010-12-31</wps:LiteralData>
            </wps:Data>
        </wps:Input>

        <wps:Input>
            <ows:Identifier>recordSourceUrl</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>hdfs://master00:9000/calvalus/projects/example/cities.txt</wps:LiteralData>
            </wps:Data>
        </wps:Input>
        <wps:Input>
            <ows:Identifier>macroPixelSize</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>3</wps:LiteralData>
            </wps:Data>
        </wps:Input>
        <wps:Input>
            <ows:Identifier>filteredMeanCoeff</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>0.0</wps:LiteralData>
            </wps:Data>
        </wps:Input>

    </wps:DataInputs>
</wps:Execute>