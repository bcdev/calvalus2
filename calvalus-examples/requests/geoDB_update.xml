<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<wps:Execute service="WPS"
             version="1.0.0"
             xmlns:wps="http://www.opengis.net/wps/1.0.0"
             xmlns:ows="http://www.opengis.net/ows/1.1"
             xmlns:xlink="http://www.w3.org/1999/xlink">

    <ows:Identifier>GeoDB</ows:Identifier>

    <wps:DataInputs>
        <wps:Input>
            <ows:Identifier>productionName</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>example GeoDB - update index from existing scans</wps:LiteralData>
            </wps:Data>
        </wps:Input>
        
        <wps:Input>
            <ows:Identifier>action</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>update</wps:LiteralData>
            </wps:Data>
        </wps:Input>

        <wps:Input>
            <ows:Identifier>geoInventory</ows:Identifier>
            <wps:Data>
                <wps:LiteralData>/calvalus/home/marcoz/geodb-tests2017</wps:LiteralData>
            </wps:Data>
        </wps:Input>

    </wps:DataInputs>
</wps:Execute>