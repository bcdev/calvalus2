<bundleDescriptor>
    <bundleName>megs</bundleName>
    <bundleVersion>8.0</bundleVersion>

    <processorDescriptors>
        <processorDescriptor>
            <executableName>megs</executableName>
            <processorName>MEGS L2 Processing</processorName>
            <processorVersion>8.0</processorVersion>
            <defaultParameters><![CDATA[]]></defaultParameters>
            <outputFormats>N1</outputFormats>
            <descriptionHtml>
                <![CDATA[<p>Performs MEGS processing.</p>]]>
            </descriptionHtml>
            <formatting>IMPLICIT</formatting>
            <inputProductTypes>MERIS_L1B</inputProductTypes>
            <outputProductType>MERIS_L2</outputProductType>
            <validMaskExpression>true</validMaskExpression>

            <jobConfig>
                <jobParameter>
                    <name>calvalus.hadoop.mapred.max.map.failures.percent</name>
                    <value>10</value>
                </jobParameter>
            </jobConfig>

            <outputVariables>
                <outputVariable>
                    <name>reflec_1</name>
                </outputVariable>
                <outputVariable>
                    <name>reflec_2</name>
                </outputVariable>
                <outputVariable>
                    <name>reflec_3</name>
                </outputVariable>
                <outputVariable>
                    <name>reflec_4</name>
                </outputVariable>
                <outputVariable>
                    <name>reflec_5</name>
                </outputVariable>
                <outputVariable>
                    <name>reflec_6</name>
                </outputVariable>
                <outputVariable>
                    <name>reflec_7</name>
                </outputVariable>
                <outputVariable>
                    <name>reflec_8</name>
                </outputVariable>
                <outputVariable>
                    <name>reflec_9</name>
                </outputVariable>
                <outputVariable>
                    <name>reflec_10</name>
                </outputVariable>
                <outputVariable>
                    <name>reflec_12</name>
                </outputVariable>
                <outputVariable>
                    <name>reflec_13</name>
                </outputVariable>
                <outputVariable>
                    <name>reflec_14</name>
                </outputVariable>
                <outputVariable>
                    <name>water_vapour</name>
                </outputVariable>
                <outputVariable>
                    <name>algal_1</name>
                </outputVariable>
                <outputVariable>
                    <name>algal_2</name>
                </outputVariable>
                <outputVariable>
                    <name>yellow_subs</name>
                </outputVariable>
                <outputVariable>
                    <name>total_susp</name>
                </outputVariable>
                <outputVariable>
                    <name>photosyn_rad</name>
                </outputVariable>
                <outputVariable>
                    <name>toa_veg</name>
                </outputVariable>
                <outputVariable>
                    <name>boa_veg</name>
                </outputVariable>
                <outputVariable>
                    <name>rect_refl_red</name>
                </outputVariable>
                <outputVariable>
                    <name>rect_refl_nir</name>
                </outputVariable>
                <outputVariable>
                    <name>surf_press</name>
                </outputVariable>
                <outputVariable>
                    <name>aero_alpha</name>
                </outputVariable>
                <outputVariable>
                    <name>aero_opt_thick_443</name>
                </outputVariable>
                <outputVariable>
                    <name>aero_opt_thick_500</name>
                </outputVariable>
                <outputVariable>
                    <name>aero_opt_thick_865</name>
                </outputVariable>
                <outputVariable>
                    <name>cloud_albedo</name>
                </outputVariable>
                <outputVariable>
                    <name>cloud_opt_thick</name>
                </outputVariable>
                <outputVariable>
                    <name>cloud_top_press</name>
                </outputVariable>
                <outputVariable>
                    <name>cloud_type</name>
                </outputVariable>
                <outputVariable>
                    <name>l2_flags</name>
                </outputVariable>
            </outputVariables>
            <parameterDescriptors>
                <parameterDescriptor>
                    <name>do_land_branch</name>
                    <type>boolean</type>
                    <description>Do all the land branch processing.</description>
                    <defaultValue>true</defaultValue>
                </parameterDescriptor>
                <parameterDescriptor>
                    <name>do_water_branch</name>
                    <type>boolean</type>
                    <description>Do all the water branch processing.</description>
                    <defaultValue>true</defaultValue>
                </parameterDescriptor>
                <parameterDescriptor>
                    <name>do_cloud_branch</name>
                    <type>boolean</type>
                    <description>Do all the cloud branch processing.</description>
                    <defaultValue>true</defaultValue>
                </parameterDescriptor>
                <parameterDescriptor>
                    <name>user_wind_u</name>
                    <type>boolean</type>
                    <description>Use user given environment data (wind_u) for Level 2 instead of ECMWF.</description>
                    <defaultValue>false</defaultValue>
                </parameterDescriptor>
                <parameterDescriptor>
                    <name>wind_u</name>
                    <type>string</type>
                    <description>User value for zonal wind (Range [-100,100]).</description>
                    <defaultValue>3</defaultValue>
                </parameterDescriptor>
                <parameterDescriptor>
                    <name>user_wind_v</name>
                    <type>boolean</type>
                    <description>Use user given environment data (wind_v) for Level 2 instead of ECMWF.</description>
                    <defaultValue>false</defaultValue>
                </parameterDescriptor>
                <parameterDescriptor>
                    <name>wind_v</name>
                    <type>string</type>
                    <description>User value for meridional wind (Range [-100,100]).</description>
                    <defaultValue>4</defaultValue>
                </parameterDescriptor>
                <parameterDescriptor>
                    <name>user_pressure</name>
                    <type>boolean</type>
                    <description>Use user given environment data (pressure) for Level 2 instead of ECMWF.</description>
                    <defaultValue>false</defaultValue>
                </parameterDescriptor>
                <parameterDescriptor>
                    <name>pressure</name>
                    <type>string</type>
                    <description>User value for surface atmospheric pressure (Range [100,2000]).</description>
                    <defaultValue>1013.25</defaultValue>
                </parameterDescriptor>
                <parameterDescriptor>
                    <name>user_ozone</name>
                    <type>boolean</type>
                    <description>Use user given environment data (ozone) for Level 2 instead of ECMWF.</description>
                    <defaultValue>false</defaultValue>
                </parameterDescriptor>
                <parameterDescriptor>
                    <name>ozone</name>
                    <type>string</type>
                    <description>User value for total ozone content (Range [0,1000]).</description>
                    <defaultValue>350.0</defaultValue>
                </parameterDescriptor>
            </parameterDescriptors>
        </processorDescriptor>
    </processorDescriptors>
</bundleDescriptor>
