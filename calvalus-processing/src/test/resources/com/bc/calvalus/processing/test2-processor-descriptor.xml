<processorDescriptor>
    <executableName>CoastColour.L2W</executableName>
    <processorName>MERIS CoastColour</processorName>
    <defaultParameters>
        <![CDATA[
<parameters>
    <useIdepix>false</useIdepix>
    <landExpression>l1_flags.LAND_OCEAN</landExpression>
    <outputReflec>false</outputReflec>
</parameters>
    ]]>
    </defaultParameters>
    <outputFormats>BEAM-DIMAP,NetCDF,GeoTIFF</outputFormats>
    <descriptionHtml>
        <![CDATA[
<p>
<h1>This is a cool processor</h1>
done by the cool folks at Brockmann-Consult.
</p>
      ]]>
    </descriptionHtml>
    <inputProductTypes>MERIS_L1B,MERIS_CC_L1P</inputProductTypes>
    <outputProductType>MERIS_CC_L2R</outputProductType>
    <outputVariableDefaultMaskExpr>!l1_flags.INVALID</outputVariableDefaultMaskExpr>
    <outputVariables>
        <outputVariable>
            <name>l1_flags</name>
        </outputVariable>
        <outputVariable>
            <name>chl_conc</name>
            <defaultAggregator>AVG</defaultAggregator>
            <defaultWeightCoeff>0.5</defaultWeightCoeff>
        </outputVariable>
    </outputVariables>
    <jobConfig>
        <jobParameter>
            <name>calvalus.system.param1</name>
            <value>value1</value>
        </jobParameter>
        <jobParameter>
            <name>calvalus.system.param2</name>
            <value>value2</value>
        </jobParameter>
    </jobConfig>
    <parameterDescriptors>
        <parameterDescriptor>
            <name>doSmile</name>
            <type>boolean</type>
            <description>Correct smile effect</description>
            <defaultValue>true</defaultValue>
        </parameterDescriptor>
        <parameterDescriptor>
            <name>mode</name>
            <type>string</type>
            <description>Correction mode</description>
            <defaultValue>simple</defaultValue>
            <valueSet>simple,complex</valueSet>
        </parameterDescriptor>
    </parameterDescriptors>
</processorDescriptor>