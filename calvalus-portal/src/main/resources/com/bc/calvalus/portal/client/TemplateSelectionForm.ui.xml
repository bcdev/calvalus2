<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent" >
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:style src="style.css" type='com.bc.calvalus.portal.client.CalvalusStyle'/>

    <g:HorizontalPanel spacing="0">
        <g:VerticalPanel spacing="0">
            <!-- <g:HTMLPanel styleName="{style.panelTitle}">
                <g:InlineLabel styleName="{style.panelTitleText}">Templates to pre-fill the form</g:InlineLabel>
            </g:HTMLPanel> -->
            <g:FlowPanel styleName="{style.centeredHorizontalPanel} {style.flexRow} {style.outputProductionName}">
                <g:VerticalPanel width="2em"/>
                <g:ListBox ui:field="requestList" width="35em" visibleItemCount="3"/>
                <g:VerticalPanel width="1em"/>
                <g:Button ui:field='editButton' text="Fill form"/>
            </g:FlowPanel>
        </g:VerticalPanel>
    </g:HorizontalPanel>

</ui:UiBinder>